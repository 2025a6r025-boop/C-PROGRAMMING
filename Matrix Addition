#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int rows,cols;
    scanf("%d %d",&rows,&cols);
    int matrix1[rows][cols],matrix2[rows][cols],result[rows][cols];
    for(int i=0;i<rows;i++){
        for(int j=0;j<cols;j++){
            scanf("%d",&matrix1[i][j]);
        }
    }
    for(int i=0;i<rows;i++){
        for(int j=0;j<cols;j++){
            scanf("%d",&matrix2[i][j]);
        }
    }
    for(int i=0;i<rows;i++){
        for(int j=0;j<cols;j++){
            result[i][j]=matrix1[i][j]+matrix2[i][j];
        }
    }
    for(int i=0;i<rows;i++){
        for(int j=0;j<cols;j++){
            printf("%d ",result[i][j]);
        }
        printf("\n");
    }
    return 0;
}
