#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int rows,cols;
    scanf("%d %d",&rows,&cols);
    int arr[rows][cols];
    for(int i=0;i<rows;i++){
        for(int j=0;j<cols;j++){
            scanf("%d",&arr[i][j]);
        }
    }
    for(int i=0;i<rows;i++){
        int rowsum=0;
       for(int j=0;j<cols;j++){
           rowsum+=arr[i][j];
       } 
        printf("%d\n",rowsum);
    }
    return 0;
}
