#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
      int smallest=arr[0];
    int largest=arr[0];
    for(int i=1;i<n;i++){
        if(arr[i]>largest){
            largest=arr[i];
        }
            if(arr[i]<smallest){
                smallest=arr[i];
            }
       
    }
    printf("%d %d",smallest,largest);
    return 0;
}
