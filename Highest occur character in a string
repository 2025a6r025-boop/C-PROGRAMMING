#include<stdio.h>
#include<stdlib.h>
#include<math.h>
#include<string.h>
int main(){
    char string[1000];
    scanf(" %[^\n]",string);
    int max=0,index=-1;
    for(int i=0;string[i]!='\0';i++){
        int count =0;
        for(int j=0;string[j]!='\0';j++){
            if(string[i]==string[j])
            count++;
        }
        if(count>max){
            max=count;
        index=i;
        }
    }
    printf("%c occurs %d times",string[index],max);
    return 0;
}
