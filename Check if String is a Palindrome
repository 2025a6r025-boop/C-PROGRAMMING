#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<math.h>
#include<ctype.h>
int main(){
    char S[1000],rstring[1000];
    scanf(" %[^\n]",S);
    for(int i=0;S[i]!='\0';i++){
        S[i]=tolower(S[i]);
    }
    int k=0;
    for(int i=strlen(S)-1;i>=0;i--){
        rstring[k++]=S[i];
    }
    rstring[k++]='\0';
    if(strcmp(S,rstring)==0){
        printf("Palindrome");
    }
    else{
        printf("Not Palindrome");
    }
    return 0;
}
