#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<math.h>
#include<ctype.h>
int main(){
    int count=0,inword=0;
    char str[1999];
    scanf(" %[^\n]",str);
    for(int i=0;str[i]!='\0';i++){
        if(isalpha(str[i])){
        if(!inword){
            count++;
            inword=1;
        }    
        }
        else{
            inword=0;
        }
    }
    printf("%d\n",count);
    return 0;
}
