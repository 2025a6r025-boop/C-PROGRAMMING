#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int size;
    scanf("%d",&size);
    int stu[size];
    for(int i=0;i<size;i++){
        scanf("%d",&stu[i]);
    }
    int sum=0;
    for(int i=0;i<size;i++){
        sum+=stu[i];
    }
    printf("%d\n",sum); 
    return 0;
}
