#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

  int num;
    scanf("%d",&num);
    if(num>=0 && num<=9){
        printf("%d",num);
        
    }
    else {
        int reverse=0;
        while(num!=0){
            int rem=num%10;
            reverse=reverse*10+rem;
            num=num/10;
        }
        printf("%d",reverse);
    }
    return 0;
}
